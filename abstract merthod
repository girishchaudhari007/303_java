abstract class X
{
	int i = 10;
	public abstract void display();	//abstract method
}
class Y extends X
{
	int i = 20;
	public void display()
	{
		System.out.println("hello");
	}
}
class abs
{
		public static void main(String args[])
		{
			Y d = new Y();
			d.display();
			System.out.println(" d.i"  + d.i);
			X a = new Y();	//reference of abstract class
			a.display();
			System.out.println(" a.i " + a.i);
	}
}
